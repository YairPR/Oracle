--Matar procesos
KILL
ps -ef | grep LOCAL=NO | awk '{print $2}' | xargs /usr/bin/kill -9
kill -9 `ps -ef | grep LOCAL=NO | grep $ORACLE_SID | grep -v grep | awk '{print $2}'`

